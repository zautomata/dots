#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main(){
        int n;
        int k;
        scanf("%d %d",&n,&k);
        printf("%i%i\n",n,k);
        int *a = malloc(sizeof(int) * n);
        for(int a_i = 0;
            scanf("%d",&a[a_i]);
            a_i++)
        {
                for (int j = 0; j < k; j++) {
                        //&a[(n-1)-j] =
                        scanf("%d",&a[(n-1)-j]);
                        printf("%i\n",a[(n-1)-j]);
                        //printf("%i" , a[i]);
                }
                scanf("%d",&a[a_i]);
        }

        printf("\nprint result\n");
        for (int i = 0; i < n; ++i) {
                printf("%i\n", a[i]);
        }
        return 0;
}
